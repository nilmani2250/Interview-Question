Write a program in sum f array?

#include<stdio.h>
void main()
{
  int a[5];
  int ele,i,sum=0;
  ele=sizeof(a)/sizeof(a[0]);
  printf("Enter the array\n");
  for(i=0;i<ele;i++)
  scanf("%d",&a[i]);
  for(i=0;i<ele;i++)
  {
      sum=sum+a[i];
   }
   printf("sum of array:%d\n",sum);
   
 }
 
 
 Write a program array of duplicate number?
 
 #include<stdio.h>
 void main()
 {
    int a[5];
    int ele,i,j;
    ele=sizef(a)/sizeof(a[0]);
    printf("Enter the array\n");
    for(i=0;i<ele;i++)
    scanf("%d",&a[i]);
    printf("Dupilicate number of array\n");
    
    for(i=0;i<ele;i++)
      {
        for(j=i+1;j<ele;j++)
          {
            if(a[i]==a[j])
              {
              printf("%d\n",a[i]);
              }
           }
        }
     }
    
    
    
   Write a program array of largest number?
   #include<stdio.h>
   void main()
   {
        int a[5];
        int ele,i,l;
        ele=sizeof(a)/sizeof(a[0]);
        printf("Enter the array\n");
        for(i=0;i<ele;i++)
        scanf("%d",&a[i]);
        l=a[i];
        for(i=0;i<ele;i++)
        {
        if(a[i] > l)
        {
          l=a[i];
        }
        }
        printf("Largest number:%d\n",l);
    }
    
    
    Write a program array of smallest number?
    #include<stdio.h>
     void main()
   {
        int a[5];
        int ele,i,s;
        ele=sizeof(a)/sizeof(a[0]);
        printf("Enter the array\n");
        for(i=0;i<ele;i++)
        scanf("%d",&a[i]);
        s=a[0];
        for(i=0;i<ele;i++)
        {
        if(a[i] < s)
        {
          s=a[i];
        }
        }
        printf("sallest number:%d\n",s);
    }
    
  Write a program array reverses with funcction?
  
  #include<stdio.h>
  void array(int *,int);
  void reverse(int *,int);
  void main()
  {
      int a[5];
      int ele,i;
      ele=sizeof(a0/sizeof(a[0]);
      printf("Enter the array\n");
      for(i=0;i<ele;i++)
      scanf("%d",&a[i]);
      
      printf("Before reverse\n");
      array(a,ele);
      reverse(a,ele);
      printf('After reverse\n");
      array(a,ele);
   }
   void reverse(int *a,int ele)
   {
      int i,j,t;
      for(i=0,j=ele-1;i<j;i++,j--)
      {
        t=a[i];
        a[i]=a[j];
        a[j]=t;
      }
    }
    void array(int *a ,int ele)
    {
      int i;
      for(i=0;i<ele;i++)
      printf("%d\n",a[i]);
    }
    
    
    
    
    
    
    
 Write a program bubble sort 
  
  #include<stdio.h>
  void main()
  {
      int a[10];
      int ele,i,j,t;
      ele=sizeof(a)/sizeof(a[0]);
      printf("Enter the array\n");
      for(i=0;i<ele;i++)
      scanf("%d",a+i);
      
      printf("Before sorting\n");
      for(i=0;i<ele;i++)
      printf("%d",a[i]);
      printf("\n");
      
        for(i=0;i<ele-1;i++)
        {
            for(j=0;j<ele-1-i;j++)
              {
                if(a[j] > a[j+1])
                  {
                    t=a[j];
                    a[j]=a[j+1];
                    a[j+1]=t;
                  }
                } 
               }
               
      printf("After sorting\n");
      for(i=0;i<ele;i++)
      printf("%d",a[i]);
      printf("\n");
  }
  
  
  Write a program string reverse with pointer
  
  #include<stdio.h>
  void reverse(char *);
  void main()
  {
      char str[100];
      printf("Enter the string\n");
      scanf("%s",str);
      reverse(str);
   }
   void reverse(char *p)
   {
     int i,c=0;
     for(i=0;*(p+i)!='\0';i++)
     {
      c++;
     }
     printf("String reverse\n");
     for(i=c-1;i>=0;i--)
     {
        printf("%c",*(p+i));
      }
    }
    
    Write a program string sorting?
    
    #include<stdio.h>
    void main()
    {
      char c[100],ch;
      int i,j,n=0;
      printf("Enter the string\n");
      scanf("%s",c);
      n=strlen(c);
      
      for(i=0;i<n-1;i++)
      {
        for(j=0;j<n-1-i;j++)
        {
          if(c[j] > c[j+1])
          {
              ch=c[j];
              c[j]=c[j+1];
              c[j+1]=ch;
           }
          }
       }
       
     printf("%s",c);
   }
   
